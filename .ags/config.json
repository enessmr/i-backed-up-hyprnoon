  "search": {
    "watchers": [
      "/usr/share/applications",
      "~/.local/share/applications",
      "/var/lib/flatpak/exports/share"
    ],
    "enableFeatures": {
      "actions": true,
      "commands": true,
      "mathResults": true,
      "directorySearch": true,
      "aiSearch": true,
      "webSearch": true
    },
    "engineBaseUrl": "https://www.google.com/search?q=",
    "excludedSites": ["quora.com"]
  },
  "timers": {
    "presets": [
      {
        "name": "Pomodoro",
        "duration": 1500,
        "icon": "timer"
      },
      {
        "name": "Short Break",
        "duration": 300,
        "icon": "coffee"
      },
      {
        "name": "Long Break",
        "duration": 900,
        "icon": "self_improvement"
      }
    ]
  },
  "time": {
    "format": "%I:%M",
    "interval": 5000,
    "dateFormatLong": "%A, %d/%m",
    "dateInterval": 5000,
    "dateFormat": "%d/%m",
    "verticalCLock": {
      "hours": "%I",
      "minutes": "%M",
      "seconds": "%S",
